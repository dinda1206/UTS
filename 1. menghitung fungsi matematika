#include <iostream>

using namespace std;

int main() {
    
    int n,i;
    cout << "Masukkan jumlah n : ";
    cin >> n;
    for(i=0; i<=n; i++){
        if(i==0){
            i=i+2;
            cout<<"F(X) = 1 ";
        }
        if(i%2==0){
            cout << "-1/" << i << " ";
        }else{
            cout << "+1/" << i << " ";
        }
    }
    
}
